.dmypy.json
dmypy.json

# Distribution / packaging
dist/
build/
*.egg-info/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# Logs
*.log
logs/
*.log.*

# Environment variables
.env
.env.local
.env.*.local
.vps_config

# SSH keys
*.pem
*.key
